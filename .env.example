# Strava API Configuration
STRAVA_CLIENT_ID=your_strava_client_id_here
STRAVA_CLIENT_SECRET=your_strava_client_secret_here
STRAVA_REDIRECT_URI=https://your-app-name.onrender.com/callback

# LLM API Configuration

# Groq Configuration
GROQ_API_KEY=your_groq_api_key_here
GROQ_DEFAULT_MODEL=llama-3.3-70b-versatile
GROQ_MODELS=llama-3.3-70b-versatile,llama-3.1-70b-versatile,mixtral-8x7b-32768,gemma2-9b-it

# OpenAI Configuration (optional)
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_DEFAULT_MODEL=gpt-4o
OPENAI_MODELS=gpt-4o,gpt-4o-mini,o1-mini,o1

# Google Gemini Configuration (optional)
GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_DEFAULT_MODEL=gemini-2.0-flash-exp
GEMINI_MODELS=gemini-2.0-flash-exp,gemini-1.5-pro,gemini-1.5-flash

# Rate Limiting Configuration (separate limits for each provider)
# NUM_ANALYSIS_GROQ: Maximum number of Groq analyses per IP address per day
# NUM_ANALYSIS_OPENAI: Maximum number of OpenAI analyses per IP address per day
# NUM_ANALYSIS_GEMINI: Maximum number of Gemini analyses per IP address per day
# Set to -1 to block all analyses for that provider (maintenance mode)
# Set to 0 for unlimited analyses (default behavior if not set)
# Set to N (e.g., 1, 5, 10) to allow N analyses per IP per day
# Recommended: Higher limit for Groq/Gemini (cheaper), lower for OpenAI (more expensive)
NUM_ANALYSIS_GROQ=20
NUM_ANALYSIS_OPENAI=5
NUM_ANALYSIS_GEMINI=20

# Note: You'll also need to upload your Google Sheets credentials JSON file
# File name: njmaniacs-485422-8e16104bb447.json

# WhatsApp Bot Configuration
WHATSAPP_CONTACT_NAME=Your Name Here
HEADLESS_MODE=false

# Debug Mode Configuration
# DEBUG_SKIP_LLM: When set to 'true', skips OpenAI API calls and logs prompts instead
# Useful for testing without incurring API costs
DEBUG_SKIP_LLM=false
